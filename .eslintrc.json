{
  "env": {
    "node": true,
    "es2017": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "pluing:prettier/recommended",
    "plugin:vue/recommended",
    "prettier"
  ],
  "plugins": [
    "vue",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "no-var": "error",
    "no-multi-spaces": 2
  }
}
