{
  "env": {
    "node": true,
    "es2017": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "prettier"
  ],
  "plugins": [
    "prettier",
    "vue"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-var": "error"
  }
}
